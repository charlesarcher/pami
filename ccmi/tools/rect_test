rect="{0 0 0 0 4 4 2 1}"
root="(0,0,0,0)"

for t in m d; do
	for a in reduce broadcast allreduce; do
		for c in A B C D E F; do
			./sched_test -V --rectangle --${a} "${rect}" "${root}" ${c} | \
				./sched_verify -${t} ${*}
			if [[ $? -ne 0 ]]; then exit 1; fi
		done
	done
done

VPATH		= @abs_srcdir@
SUBDIRS         =
NOTBUILT        = 
APPS            =  multicast.${EXTENSION} multicast_local.${EXTENSION} multicast_local_as.${EXTENSION} multicast_pwq.${EXTENSION} 
APPS            += #multicombine.${EXTENSION}
APPS            += #multisync.${EXTENSION} 

include @abs_top_builddir@/Build.rules

ifeq (@BUILDENV@,ade)
%.${EXTENSION}: %.c
	cp ${CRT1} .
	cp ${CRTI} .
	cp ${CRTN} .
	${CROSS_CXX} -I${INSTALL_DIR}/include -I@abs_srcdir@/../common/include ${CROSS_CXXFLAGS} $< -o $@ ${CROSS_LDFLAGS} -L${_BOSDEPS_USR_LIB_} @POELIB@ @IBMMPILIB@ 
#	${CROSS_CXX} -I${INSTALL_DIR}/include -I@abs_srcdir@/../common/include ${CROSS_CXXFLAGS} -c $< -o $@.o
	rm `basename ${CRT1}`
	rm `basename ${CRTI}`
	rm `basename ${CRTN}`
else

%.${EXTENSION}: %.c
	${CROSS_CXX} -I${INSTALL_DIR}/include ${CROSS_CXXFLAGS} $< -o $@ ${CROSS_LDFLAGS} -L../../../../${INSTALL_DIR}/lib
%.${EXTENSION}: %.cc
	${CROSS_CXX} -I${INSTALL_DIR}/include ${CROSS_CXXFLAGS} $< -o $@ ${CROSS_LDFLAGS} -L../../../../${INSTALL_DIR}/lib
%.${EXTENSION}: %.cpp
	${CROSS_CXX} -I${INSTALL_DIR}/include ${CROSS_CXXFLAGS} $< -o $@ ${CROSS_LDFLAGS} -L../../../../${INSTALL_DIR}/lib

endif

default: ${APPS}
